{
  "name": "npm_graph_component",
  "version": "2.0.0",
  "description": "Interactive 3D graph visualization component for React using Three.js and react-force-graph-3d",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp -r src/styles dist/styles && cp -r src/assets dist/assets && cp dist/index.js dist/index.mjs",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "keywords": [
    "react",
    "graph",
    "3d",
    "visualization",
    "three",
    "npm",
    "typescript"
  ],
  "author": "Pablo Brito",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "graphql": "^16.10.0",
    "react-force-graph-3d": ">=1.26.0 <2.0.0",
    "react": ">=18.0.0 <20.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.8.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.142.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "react-icons": "^5.5.0"
  }
}
